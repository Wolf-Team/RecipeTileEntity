{
    "info": {
        "name": "RecipeTileEntity Demo",
        "description": "A library that simplifies the creation of workbenches and furnaces.",
        "author": "WolfTeam",
        "version": "1.0",
        "icon": "icon.png",
        "api": "CoreEngine"
    },
    "resources": [{
        "path": "assets/res",
        "type": "resource_directory"
    }, {
        "path": "assets/gui",
        "type": "gui"
    }],
    "sources": [{
            "source": "dev",
            "target": "main.js",
            "type": "main",
            "language": "typescript"
        },
        {
            "source": "launcher.js",
            "type": "launcher",
            "language": "javascript"
        },
        {
            "source": "devlib",
            "target": "RecipeTileEntityLib.js",
            "type": "library",
            "language": "typescript"
        },
        {
            "source": "lib/*",
            "type": "library",
            "language": "javascript"
        }
    ],
    "compile": [],
    "additional": [{
        "source": "root/*",
        "targetDir": "./"
    }]
}